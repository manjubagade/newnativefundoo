<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FUNDOOAPP.views.Archive" BackgroundImage="backgroundimage">
    <ContentPage.ToolbarItems>
        <ToolbarItem Order="Primary" Priority="0" Text="Archive                                       " ></ToolbarItem>
        <ToolbarItem Order="Primary"  Icon="grids.png" ></ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <ScrollView Orientation="Vertical" 
                        VerticalOptions="StartAndExpand"
                        HeightRequest="480">
                <Grid x:Name="GridLayout"></Grid>
            </ScrollView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>